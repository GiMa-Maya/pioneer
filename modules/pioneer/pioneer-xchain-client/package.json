{
  "name": "@pioneer-platform/pioneer-xchain-client",
  "version": "8.1.38",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "npm": "npm i",
    "npm-update": "npm update",
    "test": "npm run build && node __tests__/test-module.js",
    "test-cli-export": "npm run build && node __tests__/reference-test-cli-export.js",
    "lint": "prettier --write '**/**/*.ts'",
    "start": "nodemon --watch 'src/**/*.ts' --exec ts-node __tests__node",
    "build": "tsc -p .",
    "build-watch": "tsc -w .",
    "prepublish": "npm run build",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "test-jest": "jest --coverage",
    "test:watch": "jest --coverage --watchAll",
    "build:watch": "npm run build && onchange 'src/**/*.ts' -- npm run build",
    "view:coverage": "serve coverage/lcov-report",
    "clean": "rm -rf coverage src/**/*.js src/**/*.map lib node_modules"
  },
  "dependencies": {
    "@binance-chain/javascript-sdk": "^4.1.1",
    "@pioneer-platform/pioneer-coins": "^8.1.37",
    "@pioneer-platform/pioneer-invoke": "^8.1.35",
    "@xchainjs/xchain-client": "^0.7.0",
    "@xchainjs/xchain-util": "^0.2.6",
    "bignumber.js": "^9.0.1",
    "dotenv": "^8.2.0",
    "ethers": "^5.1.4",
    "openapi-client-axios": "^3.13.1",
    "prettyjson": "^1.2.1"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/node": "^13.13.51",
    "@types/source-map-support": "^0.5.3",
    "jest": "^26.6.3",
    "onchange": "^7.1.0",
    "serve": "^11.3.2",
    "source-map-support": "^0.5.19",
    "ts-jest": "^25.4.0",
    "typescript": "^3.9.9"
  },
  "gitHead": "d0e74ea447175d6d7e9697793093f9155f0dab38"
}
