{
  "name": "@pioneer-platform/eth-e2e-liquidity-add",
  "version": "8.1.37",
  "scripts": {
    "yarn": "yarn",
    "start": "node lib/index.js",
    "build": "tsc -p .",
    "run-dev": "rm -Rf ~/.pioneer && npm run build && npm run start",
    "test": "rm -Rf ~/.pioneer && jest --silent=true --verbose --forceExit",
    "docker:build": "docker build --build-arg NPM_TOKEN=$NPM_TOKEN -t pioneer/eth-e2e-liquidity-add:latest .",
