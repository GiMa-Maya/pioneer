    "docker:tag": "docker tag pioneer/eth-e2e-sdk:latest registry.digitalocean.com/pioneer/pioneer/eth-e2e-liquidity-add:latest",
    "docker:push": "docker push registry.digitalocean.com/pioneer/pioneer/eth-e2e-liquidity-add:latest",
    "docker:push:all": "npm run docker:build && npm run docker:tag && npm run docker:push"
  },
  "dependencies": {
    "@pioneer-platform/loggerdog": "^8.1.16",
    "@pioneer-platform/midgard-client": "^8.1.27",
    "@pioneer-platform/pioneer-app": "^8.1.40",
    "@pioneer-platform/pioneer-sdk": "^8.1.41",
    "assert": "^2.0.0",
    "dotenv": "^6.2.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "typescript": "^4.2.4",
    "wait-promise": "^0.4.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
  },
